{
  "buildCommand": "npm run vercel-build",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 60
    },
    "app/api/upload/route.ts": {
      "maxDuration": 300,
      "memory": 1024
    }
  },
  "env": {
    "PRISMA_GENERATE_DATAPROXY": "false"
  },
  "limits": {
    "duration": 300
  }
}
